<%-include('../partials/header'); %> 

<main class="main main-actor">
    <section class="section section-actor container ">
        <div class="row">
        <% data.forEach(item => { %>
            <div class="item-div col-md-4">
            <div class="item-div">
                <figure class="figure fig-actor">
                    <img src="https://via.placeholder.com/200x250" alt="placeholder img" class="img-fluid" actor-img>
                    </figure>
                    <figcaption class="actor-figcap"><a href="/actor/<%= item.actor_id  %>">
                    <%= item.first_name  %>
                    <%= item.last_name  %>  
                </a>
            </figcaption>
            </div>
            <% }) %> 
            </div>
    </section>
</main>

<%- include('../partials/footer');  %> 