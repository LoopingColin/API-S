<%- include('../partials/header'); %>

    <main class="main" id="main">
        <h2><% name %></h2>
        <% data.forEach(item => { %>
            <div>
                <p>Title:
                    <a href="/film/<%= item.film_id  %> ">
                    <%= item.title  %> </a>
                </p>
                <p>Rating: <%= item.rating  %> </p>
                <p>Year: <%= item.release_year  %></p>
                <p>Description: <%= item.description  %> </p>
            </div> 
        
            
            <% }) %> 
    </main>

    <%- include('../partials/footer'); %>